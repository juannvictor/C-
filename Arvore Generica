#include<stdio.h>

int main(void)
{
  int matriz[3][3],i,j;
  int k,l,m;
  k=10;

  printf ("\nDigite o valor para os elementos da matriz\n\n");

  for (i=0; i<3; i++)
    for (j=0;j<3;j++)
    {
      printf("\nElemento [%d][%d] = ",i,j);
      scanf("%d", &matriz[i][j]);
    }
  
  for (i=0;i<3;i++){
    for(j=0;j<=3;j++)
    {
        printf("\nElemento [%d][%d] = %d",i,j, matriz[i][j]);
    }
  }
  
  for (l=0;matriz[i];l++)
    for(m=0;matriz[j];m++)
    {
      if(k>matriz[i][j])
        k = i;
    }
    
  printf("\n A linha com o menor elemento Ã© = [%d]",k);

  return(0);
}
